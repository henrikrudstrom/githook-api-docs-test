{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "metadata/label.json",
  "type": "object",
  "description": "Label",
  "additionalProperties": false,
  "properties": {
    "name": {
      "type": "string",
      "description": "Name"
    },
    "disabled": {
      "type": "boolean",
      "description": "Disabled"
    },
    "_type": {
      "type": "string",
      "description": "Sanity type of object."
    },
    "_id": {
      "type": "string",
      "description": "Unique sanity document id"
    },
    "_rev": {
      "type": "string",
      "description": "Sanity revision id"
    },
    "_createdAt": {
      "type": "string",
      "format": "date-time",
      "description": "Date the document was created"
    },
    "_updatedAt": {
      "type": "string",
      "format": "date-time",
      "description": "Dato of last document update"
    }
  },
  "required": [
    "name",
    "_type",
    "_id",
    "_rev",
    "_createdAt",
    "_updatedAt"
  ],
  "definitions": {}
}